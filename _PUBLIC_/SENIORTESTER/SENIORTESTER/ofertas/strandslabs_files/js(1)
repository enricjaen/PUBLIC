}else{className="item "+className
}if(item.imageUrl&&item.imageUrl.length>0&&config.headlineOnly===false){html+="<img src='"+item.imageUrl+"' />"
}if(item.sourceID==="poll"){html+="<h4><strong>"+item.headLine+"</strong></h4>"
