k=k.replace(/\w+$/,"")
}var n,j,a=[],i={collapsed:true,showMore:f,showLess:d.showLessText,showMoreLong:d.showMoreTextLong,showLessLong:d.showLessTextLong,className:d.className,el:c};
while(p&&p[0]){n=p.shift();
